<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from AreaService WSDL service -->

  <service name="Area" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://metacube.com"/>
      <parameter name="wsdlServiceElement" value="AreaService"/>
      <parameter name="schemaQualified" value="http://metacube.com"/>
      <parameter name="wsdlServicePort" value="Area"/>
      <parameter name="className" value="com.metacube.AreaSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Area"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getTriangleArea" qname="operNS:getTriangleArea" xmlns:operNS="http://metacube.com" returnQName="retNS:getTriangleAreaReturn" xmlns:retNS="http://metacube.com" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:side1" xmlns:pns="http://metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:side2" xmlns:pns="http://metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:side3" xmlns:pns="http://metacube.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getTriangleArea"/>

  </service>
</deployment>
